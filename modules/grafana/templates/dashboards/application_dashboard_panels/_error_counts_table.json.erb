{
  "columns": [
    {
      "text": "Current",
      "value": "current"
    },
    {
      "text": "Total",
      "value": "total"
    }
  ],
  "datasource": "Graphite",
  "editable": true,
  "error": false,
  "fontSize": "100%",
  "id": 9,
  "isNew": true,
  "links": [],
  "pageSize": null,
  "scroll": true,
  "showHeader": true,
  "sort": {
    "col": 1,
    "desc": true
  },
  "span": 6,
  "styles": [
    {
      "dateFormat": "YYYY-MM-DD HH:mm:ss",
      "pattern": "Time",
      "type": "date"
    },
    {
      "colorMode": "cell",
      "colors": [
        "rgba(50, 172, 45, 0.97)",
        "rgba(237, 129, 40, 0.89)",
        "rgba(245, 54, 54, 0.9)"
      ],
      "decimals": 0,
      "pattern": "/.*/",
      "thresholds": [
        "<%= @warning_threshold %>",
        "<%= @error_threshold %>"
      ],
      "type": "number",
      "unit": "short"
    }
  ],
  "targets": [
    {
      "refId": "B",
      "target": "alias(hitcount(stats.govuk.app.<%= @app_name %>.errors_occurred, '5m'), 'Errors')",
      "textEditor": true
    },
    {
      "target": "alias(hitcount(sumSeries(stats.*.nginx_logs.<%= @app_name %>*.http_5xx), '5m'), '5XXs')",
      "refId": "C",
      "textEditor": true
    }
  ],
  "timeFrom": "24h",
  "timeShift": null,
  "title": "Total = last 24 hours, Current = last 5min",
  "transform": "timeseries_aggregations",
  "type": "table"
}
